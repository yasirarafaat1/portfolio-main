{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "baseUrl": ".",
    "paths": {
      "std/*": ["./std/*"],
      "https://deno.land/std@0.168.0/*": ["./std/*"]
    }
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
